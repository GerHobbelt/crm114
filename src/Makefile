clean:
	-rm -f crm114
	-rm -f cssutil
	-rm -f osbf-util
	-rm -f cssdiff
	-rm -f cssmerge
	-rm -f crm114
	-rm -f crm114_gnu
	-rm -f crm114_tre
	-rm -f *.o
#   if the next line doesn't work in your version of make, 
#    don't worry.  cd down into the TRE directory and 'make clean' manually
	-( [ "$(TRE_TARFILE)" ] && cd $(TRE_TARFILE) && make clean )

install: FORCE
	$(MAKE) install_engine install_utils

